{
  "manifest_version": 2,

  "name": "Digital.gov",
  "description": "A simple tool",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/jquery-3.2.1.min.js", "content.js"]
    }
  ]
}
